<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- CircularBeadClipPathImageView会有抗锯齿，根据需求选择-->
    <declare-styleable name="CircularBeadClipPathImageView">
        <!--        弧度 四个角半径 存在此值时，则四个角的弧度设置会失效，优先读取此值-->
        <attr name="clipPathCorners" format="dimension" />
        <!--        左下角圆弧半径-->
        <attr name="clipPathBottomLeftRadius" format="dimension" />
        <!--        右下角圆弧半径-->
        <attr name="clipPathBottomRightRadius" format="dimension" />
        <!--        左上角圆弧半径-->
        <attr name="clipPathTopLeftRadius" format="dimension" />
        <!--        右上角圆弧半径-->
        <attr name="clipPathTopRightRadius" format="dimension" />
        <!--        是否是圆形imageView,此设置为true时，圆弧的设置失效-->
        <attr name="clipPathIsCircularImageView" format="boolean" />
        <!--        设置画笔颜色，默认白色-->
        <attr name="clipPathPaintColor" format="color" />
    </declare-styleable>
</resources>